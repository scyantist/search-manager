{
  "short_name": "Search Manager",
  "name": "Better Search Manager",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "React Ext"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "open-search-manager": {
      "suggested_key": {
        "default": "Ctrl+T"
      },
      "description": "Show the SearchManager"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs","storage"],
  "version": "1.0"
}